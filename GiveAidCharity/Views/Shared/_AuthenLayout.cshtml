
<!DOCTYPE html>
<html lang="en">
<head>
    <title>@ViewBag.Title</title>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1">
    <!--===============================================================================================-->
    <link rel="icon" type="image/png" href="/Content/AuthenTheme/images/icons/favicon.ico" />
    <!--===============================================================================================-->
    @Scripts.Render("~/bundles/modernizr")
    @Styles.Render("~/Content/Site.css")
    @Styles.Render("~/Content/bootstrap")
    <!--===============================================================================================-->
    <link rel="stylesheet" type="text/css" href="/Content/AuthenTheme/fonts/font-awesome-4.7.0/css/font-awesome.min.css">
    <!--===============================================================================================-->
    <link rel="stylesheet" type="text/css" href="/Content/AuthenTheme/fonts/iconic/css/material-design-iconic-font.min.css">
    <!--===============================================================================================-->
    <link rel="stylesheet" type="text/css" href="/Content/AuthenTheme/vendor/animate/animate.css">
    <!--===============================================================================================-->
    <link rel="stylesheet" type="text/css" href="/Content/AuthenTheme/vendor/css-hamburgers/hamburgers.min.css">
    <!--===============================================================================================-->
    <link rel="stylesheet" type="text/css" href="/Content/AuthenTheme/vendor/animsition/css/animsition.min.css">
    <!--===============================================================================================-->
    <link rel="stylesheet" type="text/css" href="/Content/AuthenTheme/vendor/select2/select2.min.css">
    <!--===============================================================================================-->
    <link rel="stylesheet" type="text/css" href="/Content/AuthenTheme/vendor/daterangepicker/daterangepicker.css">
    <!--===============================================================================================-->
    <link rel="stylesheet" type="text/css" href="/Content/AuthenTheme/css/util.css">
    <link rel="stylesheet" type="text/css" href="/Content/AuthenTheme/css/main.css">
    <link rel="stylesheet" type="text/css" href="/Content/AuthenTheme/css/custom.css">
    <!--===============================================================================================-->
</head>
<body>

    <div class="limiter">
        <div class="container-login100" style="background-image: url('/Content/AuthenTheme/images/bg-01.jpg');">
            <div class="wrap-login100 p-l-55 p-r-55 p-t-65 p-b-54">
                @RenderBody()
            </div>
        </div>
    </div>


    <div id="dropDownSelect1"></div>

    <!--===============================================================================================-->
    @Scripts.Render("~/bundles/jquery")
    <!--===============================================================================================-->
    <script src="/Content/AuthenTheme/vendor/animsition/js/animsition.min.js"></script>
    <!--===============================================================================================-->
    <script src="https://cdn.jsdelivr.net/npm/popper.js@1.16.0/dist/umd/popper.min.js" integrity="sha384-Q6E9RHvbIyZFJoft+2mJbHaEWldlvI9IOYy5n3zV9zzTtmI3UksdQRVvoxMfooAo" crossorigin="anonymous"></script>
    @Scripts.Render("~/bundles/bootstrap")

    <!--===============================================================================================-->
    <script src="/Content/AuthenTheme/vendor/select2/select2.min.js"></script>
    <!--===============================================================================================-->
    <script src="/Content/AuthenTheme/vendor/daterangepicker/moment.min.js"></script>
    <script src="/Content/AuthenTheme/vendor/daterangepicker/daterangepicker.js"></script>
    <!--===============================================================================================-->
    <script src="/Content/AuthenTheme/vendor/countdowntime/countdowntime.js"></script>
    <!--===============================================================================================-->
    <script src="/Content/AuthenTheme/js/main.js"></script>
    <script src="/Content/AuthenTheme/js/custom.js"></script>
    @RenderSection("scripts", required: false)

</body>
</html>
