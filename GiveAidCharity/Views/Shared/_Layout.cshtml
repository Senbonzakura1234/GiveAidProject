                        <!-- Link back to Colorlib can't be removed. Template is licensed under CC BY 3.0. -->
                    </p>
                </div>
            </div>
        </div>
    </footer>



    <!-- loader -->
    <div id="ftco-loader" class="show fullscreen"><svg class="circular" width="48px" height="48px"><circle class="path-bg" cx="24" cy="24" r="22" fill="none" stroke-width="4" stroke="#eeeeee" /><circle class="path" cx="24" cy="24" r="22" fill="none" stroke-width="4" stroke-miterlimit="10" stroke="#F96D00" /></svg></div>
    @Scripts.Render("~/bundles/jquery")
    @Scripts.Render("~/bundles/bootstrap")
    @Scripts.Render("~/Content/HomepageTheme/js/jquery-migrate-3.0.1.min.js")
    @Scripts.Render("~/Content/HomepageTheme/js/popper.min.js")
    @Scripts.Render("~/Content/HomepageTheme/js/bootstrap.min.js")
    @Scripts.Render("~/Content/HomepageTheme/js/jquery.easing.1.3.js")
    @Scripts.Render("~/Content/HomepageTheme/js/jquery.waypoints.min.js")

    @Scripts.Render("~/Content/HomepageTheme/js/jquery.stellar.min.js")
    @Scripts.Render("~/Content/HomepageTheme/js/owl.carousel.min.js")
    @Scripts.Render("~/Content/HomepageTheme/js/jquery.magnific-popup.min.js")
    @Scripts.Render("~/Content/HomepageTheme/js/aos.js")

    @Scripts.Render("~/Content/HomepageTheme/js/jquery.animateNumber.min.js")
    @Scripts.Render("~/Content/HomepageTheme/js/bootstrap-datepicker.js")
    @Scripts.Render("~/Content/HomepageTheme/js/jquery.timepicker.min.js")
    @Scripts.Render("~/Content/HomepageTheme/js/scrollax.min.js")

    @Scripts.Render("~/Content/HomepageTheme/js/main.js")
    @Scripts.Render("~/Content/HomepageTheme/js/custom.js")
    @RenderSection("scripts", required: false)

</body>
</html>